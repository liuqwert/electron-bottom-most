{
  "name": "electron-bottom-most",
  "version": "0.3.0",
  "description": "Put an electron window bottom-most (set the window to the bottom of the z-index)",
  "gypfile": true,
  "main": "./build/Release/bottom-most",
  "license": "MIT",
  "scripts": {
    "electron-rebuild": "./node_modules/.bin/electron-rebuild -f -w electron-bottom-most",
    "compile": "node-gyp rebuild & npm run electron-rebuild",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/armaldio/electron-bottom-most"
  },
  "dependencies": {
    "nan": "^2.13.2",
    "node-gyp": "^7.1.0"
  },
  "devDependencies": {
    "electron": "^11.2.2",
    "electron-rebuild": "^2.2.0"
  }
}
